{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "trailing-comma": [false],
    "quotemark": [true, "single", "jsx-double"],
    "no-var-requires": false,
    "no-console": false,
    "interface-name": [false, "never-prefix"],
    "arrow-parens": false,
    "interface-over-type-literal": false
  },
  "rulesDirectory": []
}

